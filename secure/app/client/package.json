  "scripts": {
    "dev": "vite --host",
    "build": "vite build --sourcemap=false",
    "build:watch": "vite build --watch",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives src",
    "lint:fix": "eslint --fix --ext \".js,.ts,.tsx\" --report-unused-disable-directives src",
    "start": "vite preview",
    "test": "TZ=Asia/Kolkata jest",
    "test:ci": "TZ=Asia/Kolkata jest -i --ci --reporters=default --reporters=jest-junit",
    "test:watch": "TZ=Asia/Kolkata jest --watch",
    "typecheck": "tsc --noEmit",
    "find-deadcode": "ts-prune | grep -v '(used in module)'"
