{
  "name": "@whylabs/siren",
  "version": "0.1.0",
  "private": true,
  "description": "Microservice for handling WhyLabs events",
  "main": "dist/app.js",
  "engines": {
    "node": ">=16.9.0"
  },
  "resolutions": {
    "minimist": "^1.2.6"
  },
  "scripts": {
    "test": "dotenv -e .env jest",
    "build": "tsc",
    "build:container": "yarn && yarn build && docker build --pull -t siren:dev .",
    "start": "yarn start:dev",
    "start:dev": "node --inspect -r dotenv/config -r ts-node/register src/app.ts | pino-pretty",
    "start:prod": "AWS_PROFILE=Production-account.DeveloperFullAccess AWS_SDK_LOAD_CONFIG=1 node --inspect -r dotenv/config -r ts-node/register src/app.ts dotenv_config_path=.env.production | pino-pretty",
    "start:container": "yarn build:container && docker run -it --rm -e NODE_ENV=local -e AWS_PROFILE=222222222222_DeveloperFullAccess --env-file .env -v ${HOME}/.aws/:/root/.aws/:ro siren:dev",
    "lint": "eslint \"{src,test}/**/*.{ts,js}?(x)\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,html,variables,overrides,*rc}\"",
    "depcheck": "yarn dlx depcheck"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.360.0",
    "@aws-sdk/client-secrets-manager": "^3.360.0",
    "@aws-sdk/client-ses": "^3.360.0",
    "@aws-sdk/client-sqs": "^3.360.0",
    "@aws-sdk/client-sts": "^3.360.0",
    "@aws-sdk/s3-request-presigner": "^3.473.0",
    "@pagerduty/pdjs": "^2.2.4",
    "@slack/webhook": "^5.0.3",
    "@whylabs/songbird-node-client": "^0.1.918",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "handlebars": "^4.7.7",
    "node-cache": "^5.1.2",
    "node-json-transform": "^1.1.2",
    "pino": "^8.0.0",
    "pino-pretty": "^10.3.0",
    "stream-to-promise": "^3.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@slack/types": "^1.10.0",
    "@types/aws-lambda": "^8.10.130",
    "@types/express": "^4.17.21",
    "@types/jest": "^28.1.1",
    "@types/node": "^14.10.1",
    "@types/pino": "^6.0.1",
    "@types/stream-to-promise": "^2.2.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.17.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "jest-jasmine2": "^28.1.1",
    "pino-colada": "^2.2.2",
    "prettier": "^2.1.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "author": "WhyLabs",
  "packageManager": "yarn@3.2.1"
}
